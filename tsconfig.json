{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@smart-address/core": ["packages/core/src/address.ts"],
      "@smart-address/core/schema": ["packages/core/src/schema.ts"],
      "@smart-address/integrations": ["packages/integrations/src/http.ts"],
      "@smart-address/integrations/nominatim": ["packages/integrations/src/nominatim.ts"],
      "@smart-address/integrations/here": ["packages/integrations/src/here.ts"],
      "@smart-address/integrations/rate-limit": ["packages/integrations/src/rate-limit.ts"],
      "@smart-address/integrations/service-client": ["packages/integrations/src/service-client.ts"],
      "@smart-address/rpc": ["packages/rpc/src/suggest.ts"],
      "@smart-address/rpc/client": ["packages/rpc/src/client.ts"],
      "@smart-address/rpc/suggest": ["packages/rpc/src/suggest.ts"],
      "@smart-address/rpc/webrtc": ["packages/rpc/src/webrtc.ts"],
      "@smart-address/sdk": ["packages/sdk/src/client.ts"]
    },
    "target": "ES2022",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "verbatimModuleSyntax": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "incremental": true,
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "files": []
}
